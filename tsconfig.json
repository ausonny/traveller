  "compilerOptions": {
      "outDir": "./built",
      "allowJs": true,
      "target": "es6",
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "node",
      "lib": ["es6", "dom", "es2017"],
      "noEmitOnError": true,
      "sourceMap": true,
      "noImplicitReturns": true
